{
  "name": "akau-shop-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:admin": "vite --port 5174 --strictPort",

    "build": "tsc -b && vite build",
    "build:shop": "echo VITE_API_BASE_URL=$VITE_API_BASE_URL && echo VITE_API_BASE=$VITE_API_BASE && npm run _build_shop",
    "build:admin": "echo VITE_API_BASE_URL=$VITE_API_BASE_URL && echo VITE_API_BASE=$VITE_API_BASE && echo VITE_SHOP_URL=$VITE_SHOP_URL && npm run _build_admin",

    "preview": "vite preview",
    "preview:shop": "vite preview --port 4173 --strictPort --outDir dist-shop",
    "preview:admin": "vite preview --port 4174 --strictPort --outDir dist-admin"
  },
  "dependencies": {
    "dompurify": "^3.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.12.0",
    "serve": "^14.2.5",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.7.3",
    "vite": "^5.4.11"
  }
}
